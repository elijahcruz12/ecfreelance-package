{
    "name": "ecwebservices/ecframework",
    "type": "library",
    "description": "The CRM for New PHP Devs",
    "keywords": [
        "ecwebservices",
        "ecframework"
    ],
    "homepage": "https://github.com/ecwebservices/ecframework",
    "license": "MIT",
    "authors": [
        {
            "name": "ECWebServices",
            "email": "elijah@ecwebservices.net",
            "homepage": "https://ecwebservices.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ecwebservices\\ecframework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ecwebservices\\ecframework\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
